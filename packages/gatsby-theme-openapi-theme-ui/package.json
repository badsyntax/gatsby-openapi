{
  "name": "gatsby-theme-openapi-theme-ui",
  "description": "A gatsby theme plugin that renders the OpenAPI docs using theme-ui.",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "author": "Richard Willis <willis.rh@gmail.com>",
  "engines": {
    "node": ">=12.0"
  },
  "dependencies": {
    "@theme-ui/presets": "^0.3.0",
    "@theme-ui/prism": "^0.3.0",
    "@theme-ui/sidenav": "^0.3.1",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-source-openapi": "^0.0.1",
    "gatsby-theme-openapi-core": "^0.0.1",
    "openapi-types": "^1.3.5",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "remark-react": "^7.0.1",
    "theme-ui": "^0.3.1"
  },
  "peerDependencies": {
    "gatsby": "^2.24.47",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@types/theme-ui": "^0.3.6",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gatsby": "^2.24.47",
    "prettier-plugin-organize-imports": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^4.0.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-floating-promises": "error",
      "@typescript-eslint/explicit-function-return-type": "off",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "parent",
            "sibling",
            "index"
          ]
        }
      ],
      "curly": "error"
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "useTabs": false
  }
}
